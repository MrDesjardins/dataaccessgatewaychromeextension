{
  "name": "devpanel",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts-ts": "2.16.0"
  },
  "scripts": {
    "start": "REACT_APP_RUNENV=web react-scripts-ts start",
    "build": "react-scripts-ts build",
    "fullbuild": "react-scripts-ts build && cpx 'build/*' ../dist",
    "fullbuildmac": "react-scripts-ts build && cp -R build/ ../dist",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^23.1.3",
    "@types/node": "^10.5.1",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "cpx": "^1.5.0",
    "typescript": "^2.9.2"
  }
}
